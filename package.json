{
  "name": "bluejsx",
  "description": "BlueJSX - pure javascript with JSX syntax",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/bluejsx/BlueJSX.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite examples/example1",
    "build": "tsup src/index.ts --format esm --dts --minify",
    "build:example": "vite build examples/example1",
    "serve": "vite preview"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "jsx",
    "vanillajs",
    "bluejsx",
    "lightweight",
    "frontend"
  ],
  "devDependencies": {
    "sass": "^1.35.1",
    "tsup": "^4.11.2",
    "typescript": "^4.3.3",
    "vite": "^2.3.7",
    "normalize-url": ">=4.5.1"
  },
  "bugs": {
    "url": "https://github.com/bluejsx/BlueJSX/issues"
  },
  "homepage": "https://bluejsx.github.io/",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "author": "momijiichigo",
  "license": "MIT"
}
