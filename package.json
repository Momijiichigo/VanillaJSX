{
  "name": "@vanillajsx/vjsx",
  "description": "VanillaJSX - pure javascript with JSX syntax",
  "version": "0.1.5",
  "repository": {
    "type": "git",
    "url": "git://github.com/vanillajsx/VanillaJSX.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite examples/example1",
    "build": "tsup src/index.ts --format esm --dts --minify",
    "build:example": "vite build examples/example1",
    "serve": "vite preview"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "components/"
  ],
  "keywords": [
    "jsx",
    "vjsx",
    "vanillajs",
    "vanillajsx",
    "frontend"
  ],
  "tags": [
    "jsx",
    "vjsx",
    "vanillajs",
    "vanillajsx",
    "frontend"
  ],
  "devDependencies": {
    "sass": "^1.32.8",
    "tsup": "^4.11.1",
    "typescript": "^4.3.2",
    "vite": "^2.1.5"
  },
  "bugs": {
    "url": "https://github.com/vanillajsx/VanillaJSX/issues"
  },
  "homepage": "https://vanillajsx.github.io/",
  "directories": {
    "doc": "doc",
    "example": "examples",
    "lib": "lib"
  },
  "author": "momijiichigo",
  "license": "MIT"
}
